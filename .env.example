# Environment Variables Template for Multiagent-manus
# Copy this file to .env and fill in your actual values

# Application Settings
APP_NAME=Multiagent-manus
APP_VERSION=1.0.0
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-here-change-this-in-production

# AI/LLM Configuration
# OpenAI
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000

# Anthropic (Claude)
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Database Configuration
# PostgreSQL (Primary Database)
DATABASE_URL=postgresql://username:password@localhost:5432/multiagent_manus
DB_HOST=localhost
DB_PORT=5432
DB_NAME=multiagent_manus
DB_USER=username
DB_PASSWORD=password

# Redis (Caching and Sessions)
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# WhatsApp Business API Configuration
WHATSAPP_ENABLED=true
WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token
WHATSAPP_PHONE_NUMBER_ID=your-phone-number-id
WHATSAPP_BUSINESS_ACCOUNT_ID=your-business-account-id
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your-webhook-verify-token
WHATSAPP_WEBHOOK_URL=https://yourdomain.com/webhook/whatsapp

# Meta/Facebook API (for WhatsApp Business)
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret

# Payment Gateway Configuration
UPI_ENABLED=true
UPI_PROVIDER=razorpay

# Razorpay Configuration
RAZORPAY_KEY_ID=rzp_test_your-key-id
RAZORPAY_KEY_SECRET=your-key-secret
RAZORPAY_WEBHOOK_SECRET=your-webhook-secret

# Cashfree Configuration
CASHFREE_APP_ID=your-cashfree-app-id
CASHFREE_SECRET_KEY=your-cashfree-secret-key

# Paytm Configuration
PAYTM_MERCHANT_ID=your-paytm-merchant-id
PAYTM_MERCHANT_KEY=your-paytm-merchant-key
PAYTM_ENVIRONMENT=staging

# PhonePe Configuration
PHONEPE_MERCHANT_ID=your-phonepe-merchant-id
PHONEPE_SALT_KEY=your-phonepe-salt-key
PHONEPE_SALT_INDEX=1

# Merchant UPI ID for QR codes
MERCHANT_UPI_ID=yourbusiness@upi

# Social Media API Configuration
# Facebook/Instagram
FACEBOOK_PAGE_ACCESS_TOKEN=your-facebook-page-access-token
FACEBOOK_PAGE_ID=your-facebook-page-id
INSTAGRAM_BUSINESS_ACCOUNT_ID=your-instagram-business-account-id

# Twitter/X API
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_SECRET=your-twitter-api-secret
TWITTER_ACCESS_TOKEN=your-twitter-access-token
TWITTER_ACCESS_TOKEN_SECRET=your-twitter-access-token-secret
TWITTER_BEARER_TOKEN=your-twitter-bearer-token

# LinkedIn API
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret
LINKEDIN_ACCESS_TOKEN=your-linkedin-access-token

# YouTube API
YOUTUBE_API_KEY=your-youtube-api-key
YOUTUBE_CHANNEL_ID=your-youtube-channel-id

# Google Services
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REFRESH_TOKEN=your-google-refresh-token

# Google Analytics
GOOGLE_ANALYTICS_TRACKING_ID=GA-XXXXXXXXX-X
GOOGLE_ANALYTICS_PROPERTY_ID=your-ga4-property-id

# Google Ads
GOOGLE_ADS_CUSTOMER_ID=your-google-ads-customer-id
GOOGLE_ADS_DEVELOPER_TOKEN=your-google-ads-developer-token

# Google My Business
GOOGLE_MY_BUSINESS_ACCOUNT_ID=your-gmb-account-id
GOOGLE_MY_BUSINESS_LOCATION_ID=your-gmb-location-id

# Email Configuration
EMAIL_ENABLED=true
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_TLS=true
FROM_EMAIL=noreply@yourbusiness.com

# SMS Configuration
SMS_ENABLED=true
SMS_PROVIDER=aws_sns

# AWS Configuration (for SMS, S3, etc.)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=ap-south-1
AWS_S3_BUCKET=your-s3-bucket-name

# Twilio (Alternative SMS provider)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# File Storage
STORAGE_PROVIDER=local
UPLOAD_PATH=./uploads
MAX_FILE_SIZE_MB=10

# Webhook URLs
WEBHOOK_BASE_URL=https://yourdomain.com
PAYMENT_WEBHOOK_URL=https://yourdomain.com/webhook/payment
WHATSAPP_WEBHOOK_URL=https://yourdomain.com/webhook/whatsapp
SOCIAL_WEBHOOK_URL=https://yourdomain.com/webhook/social

# Security
JWT_SECRET_KEY=your-jwt-secret-key-change-this
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24
REFRESH_TOKEN_EXPIRATION_DAYS=30

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST_LIMIT=10

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=./logs/multiagent_manus.log

# Monitoring and Analytics
MONITORING_ENABLED=true
ANALYTICS_ENABLED=true

# Indian Business Specific
BUSINESS_NAME=Your Business Name
BUSINESS_ADDRESS=Your Business Address, City, State, Pincode
BUSINESS_PHONE=+91XXXXXXXXXX
BUSINESS_EMAIL=contact@yourbusiness.com
BUSINESS_WEBSITE=https://yourbusiness.com

# GST Information
GST_NUMBER=your-gst-number
GST_ENABLED=true

# Business Hours (24-hour format)
BUSINESS_START_TIME=09:00
BUSINESS_END_TIME=18:00
BUSINESS_TIMEZONE=Asia/Kolkata

# Languages
DEFAULT_LANGUAGE=hi
SUPPORTED_LANGUAGES=hi,en

# Festival Marketing
FESTIVAL_MARKETING_ENABLED=true
AUTO_FESTIVAL_CAMPAIGNS=true

# Regional Settings
BUSINESS_STATE=Maharashtra
BUSINESS_CITY=Mumbai
TARGET_REGIONS=Maharashtra,Karnataka,Gujarat

# Compliance
DATA_PRIVACY_ENABLED=true
INDIAN_DATA_PROTECTION_COMPLIANCE=true

# Development Settings
MOCK_PAYMENTS=false
MOCK_WHATSAPP=false
MOCK_SOCIAL_MEDIA=false
TEST_MODE=false

# API Configuration
API_VERSION=v1
API_BASE_PATH=/api/v1

# CORS Settings
CORS_ENABLED=true
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Health Check
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PATH=/health

# Performance
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT_SECONDS=30
DATABASE_POOL_SIZE=20

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600
CACHE_PREFIX=multiagent_manus

# Background Tasks
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Optional: Third-party Services
# Zoho CRM (popular in India)
ZOHO_CLIENT_ID=your-zoho-client-id
ZOHO_CLIENT_SECRET=your-zoho-client-secret
ZOHO_REFRESH_TOKEN=your-zoho-refresh-token

# Indian SMS Providers
# TextLocal
TEXTLOCAL_API_KEY=your-textlocal-api-key
TEXTLOCAL_SENDER=your-sender-id

# MSG91
MSG91_API_KEY=your-msg91-api-key
MSG91_SENDER_ID=your-sender-id

# Regional Directories and Services
JUSTDIAL_API_KEY=your-justdial-api-key
INDIAMART_API_KEY=your-indiamart-api-key

# Additional Configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE=daily
BACKUP_RETENTION_DAYS=30

# Performance Monitoring
APM_ENABLED=false
SENTRY_DSN=your-sentry-dsn

# Feature Flags
FEATURE_AI_INSIGHTS=true
FEATURE_AUTO_CAMPAIGNS=true
FEATURE_BULK_WHATSAPP=true
FEATURE_MULTI_LANGUAGE=true
FEATURE_FESTIVAL_CAMPAIGNS=true
